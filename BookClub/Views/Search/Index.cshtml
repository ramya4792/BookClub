@model IList<BookClub.Models.Book>

@if (Model.Count == 0)
{
    <p>No results Found</p>
}
<table class=" table">
    <tr>
        <th>Title</th>
        <th>Author</th>
        <th>Price</th>
    </tr>
    @foreach(var item in Model)
    {
        <tr>
            <td>@item.Title</td>
            <td>@item.Author</td>
            <td>@item.Price</td>
        </tr>

    }
    
</table>